{
  "name": "vdom-kv-form",
  "description": "virtual-dom component for an editable list of keys and values",
  "main": "KVForm.js",
  "version": "0.0.4",
  "keywords": [
    "mercury",
    "virtual-dom",
    "component",
    "ui"
  ],
  "browserify": {
    "transform": []
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pp": "npm version patch && npm version publish",
    "build": "browserify -d example/example.js > example/bundle.js",
    "watch": "watchify example/example.js -o example/bundle.js -dv",
    "build-css": "cp style/main.css example/style.css",
    "dev": "npm run watch & npm run server",
    "server": "node server.js",
    "postversion": "git push && git push --tags",
    "demo": "browserify example/example.js > example/bundle.js && html-inline example/index.html | curl -sT- https://htmlb.in"
  },
  "author": "Nick Thomas <nichoth@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/nichoth/vdom-kv-form.git"
  },
  "license": "ISC",
  "peerDependencies": {},
  "devDependencies": {
    "browserify": "^11.2.0",
    "ecstatic": "^1.1.3",
    "main-loop": "^3.2.0",
    "watchify": "^3.4.0",
    "virtual-dom": "^2.1.1"
  },
  "dependencies": {
    "@nichoth/state": "0.0.2",
    "observ": "^0.2.0",
    "observ-array": "^3.2.1",
    "observ-struct": "^6.0.0",
    "vdom-buttons": "0.0.2",
    "vdom-form": "^0.2.2"
  }
}
